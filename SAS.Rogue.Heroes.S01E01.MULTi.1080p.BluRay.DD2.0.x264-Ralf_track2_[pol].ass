Dialogue: 0,0:08:24.76,0:08:28.32,Default,,0,0,0,region=rg1,{\an8}BRYTYJSKIE WIĘZIENIE WOJSKOWE GHADZI,\NKAIR, TEGO SAMEGO WIECZORU
Dialogue: 0,0:08:29.52,0:08:33.28,Default,,0,0,0,region=rg2,Jeśli ferwor nienawiści\Ni pożądania w ciele silne są,
Dialogue: 0,0:08:33.36,0:08:37.15,Default,,0,0,0,region=rg2,przypomnę sobie dom z prochu,\Ngdzie zatrzymam się na dłużej.
Dialogue: 0,0:08:37.24,0:08:41.28,Default,,0,0,0,region=rg2,W narodzie, który przestał istnieć,\Nnic nie jest tak jak dawniej.
Dialogue: 0,0:08:41.36,0:08:44.84,Default,,0,0,0,region=rg2,Zemsta zapomniana,\Nnienawiść odnienawidzona.
Dialogue: 0,0:08:45.44,0:08:49.56,Default,,0,0,0,region=rg2,Tylko zegar wieszczący śmierć\Nna stryczku nadal bije.
Dialogue: 0,0:09:13.92,0:09:15.52,Default,,0,0,0,region=rg2,Jak tam kolega?
Dialogue: 0,0:09:18.64,0:09:20.76,Default,,0,0,0,region=rg2,Wyliże się?
Dialogue: 0,0:09:23.04,0:09:24.20,Default,,0,0,0,region=rg2,Irlandzki pies.
Dialogue: 0,0:09:24.28,0:09:28.92,Default,,0,0,0,region=rg2,Pies to na baby.\NŻe irlandzki to się chwali.
Dialogue: 0,0:09:29.36,0:09:33.84,Default,,0,0,0,region=rg2,A normalnie\Nto przyjaciel samotników.
Dialogue: 0,0:09:34.92,0:09:38.20,Default,,0,0,0,region=rg2,Wasz koleżka był niemiły\Ndla słabszego i to mnie wkurzyło.
Dialogue: 0,0:09:39.28,0:09:40.64,Default,,0,0,0,region=rg2,A teraz pożałujesz.
Dialogue: 0,0:09:41.96,0:09:46.08,Default,,0,0,0,region=rg2,Żandarm to nie\Nworek treningowy, czubku.
Dialogue: 0,0:09:46.92,0:09:49.60,Default,,0,0,0,region=rg2,Nikogo nie zdziwi, jak rano\Nznajdą twoje dyndające truchło.
Dialogue: 0,0:09:50.76,0:09:53.24,Default,,0,0,0,region=rg2,"Tylko zegar nadal bije".
Dialogue: 0,0:09:56.16,0:09:58.08,Default,,0,0,0,region=rg2,Jeśli mam zawisnąć,
Dialogue: 0,0:09:59.24,0:10:00.00,Default,,0,0,0,region=rg2,musicie mnie zdybać,
Dialogue: 0,0:10:00.00,0:10:01.44,Default,,0,0,0,region=rg2,musicie mnie zdybać,
Dialogue: 0,0:10:03.88,0:10:05.80,Default,,0,0,0,region=rg2,bo uwielbiam nierówne pojedynki.
Dialogue: 0,0:10:08.92,0:10:10.24,Default,,0,0,0,region=rg2,Zaczynajcie.
Dialogue: 0,0:10:10.32,0:10:12.32,Default,,0,0,0,region=rg1,{\an8}PORUCZNIK ROBERT\NBLAIR "PADDY" MAYNE,
